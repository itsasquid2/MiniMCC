variables:
	{bla} = 0
	{canplaylevels.%player%} = true
	{quartz.%player%} = 0


command /playmusic <text>:
	trigger:
		if command sender is not the console:
			if {mmcc.hoster.%command sender%} isn't true:
				message "<pink>You do not have permission to run this command!"
				exit trigger
		if arg 1 is "none":
			execute console command "/stopsound @a"
		else:
			loop all players:
				play sound "custom:%arg%" in voice category to loop-player 


command /togglemusic:
	trigger:
		if {music.%player%} is true:
			set {music.%player%} to false
			message "<lime>Toggled music off!"
			execute console command "/stopsound %player%"
		else:
			set {music.%player%} to true
			message "<lime>Toggled music on!"

