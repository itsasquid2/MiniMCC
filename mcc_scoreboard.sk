			set line 12 of {_p}'s scoreboard to ""
			set line 4 of {_p}'s scoreboard to "<yellow><bold>Your coins: <white>%{mcc.gamecoins.%{_p}%}%"
		else if {mcc.currentevent} is equal to "battlebox":	
			set line 15 of {_p}'s scoreboard to "<light blue><bold>Game %{mcc.gamenum}%/6: <white>Battle Box"
			set line 14 of {_p}'s scoreboard to "<lime><bold>Round: <white>%{mcc.bb.round}%/%{mcc.bb.roundamount}%"
			if {mcc.bb.kits} is true:
