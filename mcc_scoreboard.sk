			if {mcc.hitw.stage} is -1:
				if {mcc.hitw.timeleft} is greater than 0:
					set line 14 of {_p}'s scoreboard to "<pink><bold>Back to hub in: <white>%time({mcc.hitw.timeleft})%"
			else:
				set line 14 of {_p}'s scoreboard to {_event}
			set line 13 of {_p}'s scoreboard to "<pink><bold>Time Left: <white>%time({mcc.hitw.timeleft})%"
			set line 4 of {_p}'s scoreboard to "<lime><bold>Players remaining: <white>%{hitw.numplayers}%"
