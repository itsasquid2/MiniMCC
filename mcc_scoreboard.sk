variables:
	{mcc.currentevent} = "none"
	{mcc.eventnum} = 1
	{mcc.gamenum} = 1
	{mcc.gameplace.1} = 0
	{mcc.gameplace.2} = 0
	{mcc.gameplace.3} = 0
	{mcc.gameplace.4} = 0
	{mcc.gameplace.5} = 0
	{mcc.gameplace.6} = 0
	{mcc.gameplace.7} = 0
	{mcc.gameplace.8} = 0
	{mcc.gameplace.9} = 0
	{mcc.gameplace.10} = 0

command /seteventnum <text>:
	trigger:
		if {mmcc.hoster.%command sender%} is true:
			set {mcc.eventnum} to arg 1

function mccsp(n:number) :: text:
	if {_n} is equal to 1:
		return "        " 
	else if {_n} is equal to 2:
		return "          " 
	else if {_n} is equal to 3:
		return " " 
	else if {_n} is equal to 4:
		return "   " 
	else if {_n} is equal to 5:
		return "            " 
	else if {_n} is equal to 6:
		return "     " 
	else if {_n} is equal to 7:
		return "          " 
	else if {_n} is equal to 8:
		return "       " 
	else if {_n} is equal to 9:
		return "   " 
	else if {_n} is equal to 10:
		return "   " 


every second:
	if {mcc.inprogress} is false:
		exit trigger
	if {mcc.currentevent} is equal to "acerace":
		mccgetaceracestandings()
	else if {mcc.currentevent} is not equal to "none":
		mccgetgamestandings()
	loop all players:
		if {mcc.team.%loop-player%} is not equal to 0: 
			mccupdatescoreboard(loop-player)
		else if loop-player is "itsasquid2":
			mccupdatescoreboard(loop-player)


function mccplacenum(p:player) :: number:
	loop 10 times:
		if {mcc.gameplace.%loop-number%} is equal to {mcc.team.%{_p}%}:
			return loop-number	
	return 1

function mccgetgamestandings():
	loop 10 times:
		add {mcc.teamgamecoins.%loop-number%}*{mcc.coinmultiplier} to {_teamgamecoins::*}
		add loop-number to {_already::*}
	set {_teamgamecoins::*} to sorted {_teamgamecoins::*}
	loop numbers from 10 to 1:
		set {_good} to false
		loop 10 times:
			if {_good} is false:
				if {_already::*} contains loop-number-2:
					if {mcc.teamgamecoins.%loop-number-2%}*{mcc.coinmultiplier} is equal to {_teamgamecoins::%loop-number-1%}:
						set {mcc.gameplace.%11-(loop-number-1)%} to loop-number-2
						set {_good} to true
						remove loop-number-2 from {_already::*}

function mccgetaceracestandings():
	delete {_aceracescores::*}
	delete {_already::*}
	mccupdateaceracescores()
	set {mcc.acerace.numplayers} to 0
	loop all players:
		if {mcc.team.%loop-player%} is not equal to 0:
			if {mcc.team.%loop-player%} is less than 11:
				add 1 to {mcc.acerace.numplayers}
				add {mcc.acerace.score.%loop-player's name%} to {_aceracescores::*}
		add loop-player to {_already::*}
	set {_aceracescores::*} to sorted {_aceracescores::*}
	set {_num} to size of {_already::*}
	loop numbers from (size of {_already::*}) to 1:
		set {_good} to false
		loop all players:
			if {mcc.team.%loop-player%} is not equal to 0:
				if {_good} is false:
					if {_already::*} contains loop-player:
						if {mcc.acerace.score.%loop-player%} is equal to {_aceracescores::%loop-number%}:
							#broadcast {mcc.acerace.score.%loop-player%}
							set {mcc.acerace.place.%{_num} + 1 - loop-number-1%} to loop-player
							set {mcc.acerace.placenum.%loop-player%} to {_num} + 1 - loop-number-1
							#broadcast "%loop-player% - %{_num} + 1 - loop-number-1%"
							set {_good} to true
							remove loop-player from {_already::*}
		
	


function mccupdatescoreboard(p:player):
	if {mcc.currentevent} is equal to "none":
		clear {_p}'s scoreboard
	else if {mcc.currentevent} is equal to "intro":
		set {_bla} to 0
	else if {mcc.currentevent} is equal to "startingsoon":
		set title of {_p}'s scoreboard to "<light blue><bold>Mini MCC %{mcc.eventnum}%"
		set line 15 of {_p}'s scoreboard to "<pink><bold>Event begins in:"
		set line 14 of {_p}'s scoreboard to "Waiting..."
		set line 13 of {_p}'s scoreboard to ""
		set line 12 of {_p}'s scoreboard to "<lime><bold>Players: <reset>%player count%"
		set line 11 of {_p}'s scoreboard to ""
		set line 10 of {_p}'s scoreboard to "<bold>Your team:"
		set line 9 of {_p}'s scoreboard to "%{mcc.teams::%{mcc.team.%{_p}%}%}%"
		set line 8 of {_p}'s scoreboard to ""
		set line 7 of {_p}'s scoreboard to "<lime><bold>Last Event Coins: <reset>%{mcc.indivcoins.%{_p}%}%"
		set line 6 of {_p}'s scoreboard to "<lime>Lifetime Coins: <reset>%{mcc.lifetimecoins.%{_p}%}%"

	else if {mcc.currentevent} is equal to "decisiondome":	
		set title of {_p}'s scoreboard to "<light blue><bold>Mini MCC %{mcc.eventnum}%"
		if {mcc.decisiondome.votingtime} is greater than or equal to 55:
			set line 10 of {_p}'s scoreboard to "<pink><bold>Voting begins in:"	
			set line 9 of {_p}'s scoreboard to time({mcc.decisiondome.votingtime}-55)
		else if {mcc.decisiondome.votingtime} is greater than or equal to 25:
			set line 10 of {_p}'s scoreboard to "<pink><bold>Voting closes in:"	
			set line 9 of {_p}'s scoreboard to time({mcc.decisiondome.votingtime}-25)
			if {mcc.decisiondome.votingtime} is equal to 25:
				remove 1 egg from {_p}
		else if {mcc.decisiondome.votingtime} is greater than or equal to 10:
			set line 10 of {_p}'s scoreboard to "<pink><bold>Game chosen in:"	
			set line 9 of {_p}'s scoreboard to time({mcc.decisiondome.votingtime}-10)
		else:
			set line 10 of {_p}'s scoreboard to "<pink><bold>Teleporting to game in:"
			set line 9 of {_p}'s scoreboard to time({mcc.decisiondome.votingtime})
		set line 8 of {_p}'s scoreboard to ""
		set line 7 of {_p}'s scoreboard to "<lime><bold>Players: <white>%player count%"
		set line 6 of {_p}'s scoreboard to ""
		set line 5 of {_p}'s scoreboard to "<bold>Your team:"
		set line 4 of {_p}'s scoreboard to "%{mcc.teams::%{mcc.team.%{_p}%}%}%"
		set line 3 of {_p}'s scoreboard to ""
		set line 2 of {_p}'s scoreboard to "<lime><bold>Event coins: <white>%{mcc.indivcoins.%{_p}%}%"
		set line 1 of {_p}'s scoreboard to "<lime><bold>Lifetime coins: <white>%{mcc.lifetimecoins.%{_p}%}%"
	else if {mcc.currentevent} is equal to "break":	
		set title of {_p}'s scoreboard to "<light blue><bold>Mini MCC %{mcc.eventnum}%"
		set line 10 of {_p}'s scoreboard to "<pink><bold>Break ends in:"
		set line 9 of {_p}'s scoreboard to time({mcc.break.timeleft})
		set line 8 of {_p}'s scoreboard to ""
		set line 7 of {_p}'s scoreboard to "<lime><bold>Players: <white>%player count%"
		set line 6 of {_p}'s scoreboard to ""
		set line 5 of {_p}'s scoreboard to "<bold>Your team:"
		set line 4 of {_p}'s scoreboard to "%{mcc.teams::%{mcc.team.%{_p}%}%}%"
		set line 3 of {_p}'s scoreboard to ""
		set line 2 of {_p}'s scoreboard to "<lime><bold>Event coins: <white>%{mcc.indivcoins.%{_p}%}%"
		set line 1 of {_p}'s scoreboard to "<lime><bold>Lifetime coins: <white>%{mcc.lifetimecoins.%{_p}%}%"
	else if {mcc.currentevent} is equal to "dodgebolt":	
		set title of {_p}'s scoreboard to "<light blue><bold>Mini MCC %{mcc.eventnum}%"
		set line 15 of {_p}'s scoreboard to "<light blue><bold>Final Game:<white> Dodgebolt"
		if {mcc.db.timeleft} is equal to -1:
			set line 14 of {_p}'s scoreboard to "<pink><bold>In Game"
		else:
			set line 14 of {_p}'s scoreboard to "<pink><bold>Next round in: <white>%time({mcc.db.timeleft})%"
		set line 13 of {_p}'s scoreboard to ""
		set line 12 of {_p}'s scoreboard to "%{mcc.teams::%{db.color1}%}%: %{db.teamwinsa}%"
		set line 11 of {_p}'s scoreboard to "%{mcc.teams::%{db.color2}%}%: %{db.teamwinsb}%"
