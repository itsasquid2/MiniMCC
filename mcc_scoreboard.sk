			set line 15 of {_p}'s scoreboard to "<light blue><bold>Game %{mcc.gamenum}%/6: <white>TGTTOSAWAF"
			set line 14 of {_p}'s scoreboard to "<lime><bold>Round: <white>%{mcc.tgttosawaf.roundnum}%/6"
			if {mcc.tgttosawaf.middle} is true:
