				set line 6 of {_p}'s scoreboard to "10. <bold>%{mcc.teams::%{mcc.gameplace.10}%}% %mccsp({mcc.gameplace.10})% <white>%ceil({mcc.teamgamecoins.%{mcc.gameplace.10}%}*{mcc.coinmultiplier})%"
			else:
				set {_bla1} to mccplacenum({_p})-1
				set {_bla2} to {_bla1}+1 
				set {_bla3} to {_bla1}+2 
				set line 9 of {_p}'s scoreboard to "1. %{mcc.teams::%{mcc.gameplace.1}%}% %mccsp({mcc.gameplace.1})% <white>%ceil({mcc.teamgamecoins.%{mcc.gameplace.1}%}*{mcc.coinmultiplier})%"
				set line 8 of {_p}'s scoreboard to "%{_bla1}%. %{mcc.teams::%{mcc.gameplace.%{_bla1}%}%}% %mccsp({mcc.gameplace.%{_bla1}%})% <white>%ceil({mcc.teamgamecoins.%{mcc.gameplace.%{_bla1}%}%}*{mcc.coinmultiplier})%"
				set line 7 of {_p}'s scoreboard to "%{_bla2}%. <bold>%{mcc.teams::%{mcc.gameplace.%{_bla2}%}%}% %mccsp({mcc.gameplace.%{_bla2}%})% <white>%ceil({mcc.teamgamecoins.%{mcc.gameplace.%{_bla2}%}%}*{mcc.coinmultiplier})%"
				set line 6 of {_p}'s scoreboard to "%{_bla3}%. %{mcc.teams::%{mcc.gameplace.%{_bla3}%}%}% %mccsp({mcc.gameplace.%{_bla3}%})% <white>%ceil({mcc.teamgamecoins.%{mcc.gameplace.%{_bla3}%}%}*{mcc.coinmultiplier})%"
		if {mcc.currentevent} is equal to "hungergames":	
			set line 15 of {_p}'s scoreboard to "<light blue><bold>Game %{mcc.gamenum}%/6: <white>Hunger Games"
			if {hg.choosingkit} is greater than 0:
				set line 14 of {_p}'s scoreboard to "<pink><bold>Kit selection: <white>%time({hg.choosingkit})%"
			else:
				if {hg.time} is less than 120:
					set {_event} to "Invincibility gone"
					set {_time} to 120
				else if {hg.time} is less than 300:
					set {_event} to "Mini feast"
					set {_time} to 300
				else if {hg.time} is less than 600:
					set {_event} to "Mini feast"
					set {_time} to 600
				else if {hg.time} is less than 1080:
					set {_event} to "Feast"
					set {_time} to 1080
				else if {hg.time} is less than 1200:
					set {_event} to "Mini feast"
					set {_time} to 1200
				else if {hg.time} is less than 1500:
					set {_event} to "Bonus feast"
					set {_time} to 1500
				else if {hg.time} is less than 2000:
					set {_event} to "Deathmatch"
					set {_time} to 2000
				set {_tim} to {_time}-{hg.time}
				if {hg.backtohub} is greater than 0:
					set line 14 of {_p}'s scoreboard to "<pink><bold>Back to hub in: <white>%time({hg.backtohub})%"
				else if mod({_tim}, 60) is less than 10:
					set line 14 of {_p}'s scoreboard to "<pink><bold>%{_event}% in: <white>%floor({_tim}/60)%:0%mod({_tim},60)%"
				else:
					set line 14 of {_p}'s scoreboard to "<pink><bold>%{_event}% in: <white>%floor({_tim}/60)%:%mod({_tim},60)%"
			set line 13 of {_p}'s scoreboard to ""
			set line 4 of {_p}'s scoreboard to "<lime><bold>Players remaining: <white>%{hg.numplayers}%"
			set line 3 of {_p}'s scoreboard to "<yellow><bold>Your kills: <white>%{hg.gamekills.%{_p}%}%"
