			set line 10 of {_p}'s scoreboard to ""
			set line 5 of {_p}'s scoreboard to ""
			if mccplacenum({_p}) is equal to 1:
				set line 9 of {_p}'s scoreboard to "1. <bold>%{mcc.teams::%{mcc.gameplace.1}%}% %mccsp({mcc.gameplace.1})% <white>%ceil({mcc.teamgamecoins.%{mcc.gameplace.1}%}*{mcc.coinmultiplier})%"
				set line 8 of {_p}'s scoreboard to "2. %{mcc.teams::%{mcc.gameplace.2}%}% %mccsp({mcc.gameplace.2})% <white>%ceil({mcc.teamgamecoins.%{mcc.gameplace.2}%}*{mcc.coinmultiplier})%"
				set line 7 of {_p}'s scoreboard to "3. %{mcc.teams::%{mcc.gameplace.3}%}% %mccsp({mcc.gameplace.3})% <white>%ceil({mcc.teamgamecoins.%{mcc.gameplace.3}%}*{mcc.coinmultiplier})%"
				set line 6 of {_p}'s scoreboard to "4. %{mcc.teams::%{mcc.gameplace.4}%}% %mccsp({mcc.gameplace.4})% <white>%ceil({mcc.teamgamecoins.%{mcc.gameplace.4}%}*{mcc.coinmultiplier})%"
			else if mccplacenum({_p}) is equal to 2:
				set line 9 of {_p}'s scoreboard to "1. %{mcc.teams::%{mcc.gameplace.1}%}% %mccsp({mcc.gameplace.1})% <white>%ceil({mcc.teamgamecoins.%{mcc.gameplace.1}%}*{mcc.coinmultiplier})%"
				set line 8 of {_p}'s scoreboard to "2. <bold>%{mcc.teams::%{mcc.gameplace.2}%}% %mccsp({mcc.gameplace.2})% <white>%ceil({mcc.teamgamecoins.%{mcc.gameplace.2}%}*{mcc.coinmultiplier})%"
				set line 7 of {_p}'s scoreboard to "3. %{mcc.teams::%{mcc.gameplace.3}%}% %mccsp({mcc.gameplace.3})% <white>%ceil({mcc.teamgamecoins.%{mcc.gameplace.3}%}*{mcc.coinmultiplier})%"
				set line 6 of {_p}'s scoreboard to "4. %{mcc.teams::%{mcc.gameplace.4}%}% %mccsp({mcc.gameplace.4})% <white>%ceil({mcc.teamgamecoins.%{mcc.gameplace.4}%}*{mcc.coinmultiplier})%"
			else if mccplacenum({_p}) is equal to 10:
				set line 9 of {_p}'s scoreboard to "1. %{mcc.teams::%{mcc.gameplace.1}%}% %mccsp({mcc.gameplace.1})% <white>%ceil({mcc.teamgamecoins.%{mcc.gameplace.1}%}*{mcc.coinmultiplier})%"
