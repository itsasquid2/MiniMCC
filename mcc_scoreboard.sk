		else if {mcc.currentevent} is equal to "acerace":
			set line 15 of {_p}'s scoreboard to "<light blue><bold>Game %{mcc.gamenum}%/6: <white>Ace Race"
			set line 6 of {_p}'s scoreboard to ""
