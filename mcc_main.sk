				delete {_blabla::*}
		wait 3 seconds
		if {mcc.currentevent} is not equal to "gridrunners":
			broadcast "<bold>Top 5 players this game:"
			log "Individual scores this game:" to "mcc"
			wait 3 seconds
			loop {mccplaying::*}:
				set {mcc.already.%loop-player%} to false
